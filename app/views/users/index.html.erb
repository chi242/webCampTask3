<%= render 'userinfo', post_images: @homes %>

<div class="col-md-8 offset-md-1">
  <h2>Users</h2>
  <table class="table table-hover table-inverse">
      <thead>
        <tr>
          <th>image</th>
          <th>name</th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |users| %>
        <tr>
            <td><%= link_to users.image %></td>
            <td><%= users.name %></td>
            <td><%= link_to 'Show' , user_path(@user.id) %></td>
        </tr>
        <% end %>
      </tbody>
      </table>
    </div>